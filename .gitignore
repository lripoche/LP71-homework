# Ignore local context
/.bundle

# Ignore temporary and logs files 
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore local gems (will be downloaded for test/deploy anyway)
vendor/bundle

# Ignore uploaded files in development.
/storage/*
!/storage/.keep
/public/assets
.byebug_history

# Ignore master key for decrypting credentials and more.
/config/master.key


/node_modules
/yarn-error.log
yarn-debug.log*
.yarn-integrity

# Ignore the default SQLite database.
/db/*.sqlite3
/db/*.sqlite3-journal
/db/*.sqlite3-*